{
  "name": "touchbarvscode",
  "displayName": "TouchBar Pro",
  "description": "Support touch bar for macbook",
  "publisher": "amedomary",
  "repository": "https://github.com/Amedomary/touchbar-vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Keymaps"
  ],
  "keywords": [
    "macbook pro",
    "extra buttons",
    "vscode",
    "touch bar",
    "amedomary"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
				"command": "editor.action.goToReferences",
        "title": "→ƒ Ref"
			}, {
				"command": "workbench.action.togglePanel",
        "title": "⬓ Term"
			}, {
				"command": "editor.action.rename",
        "title": "✎ Re"
      }, {
        "command": "workbench.action.closeOtherEditors",
        "title": "⤭ Close"
      }, {
        "command": "workbench.action.toggleZenMode",
        "title": "□ Zen"
      }
    ],
    "menus": {
      "touchBar": [
				{
          "command": "editor.action.goToReferences",
          "group": "group_1",
          "when": "config.touchBar.goToReferences"
				}, {
          "command": "workbench.action.togglePanel",
          "group": "group_2",
          "when": "config.touchBar.terminal"
				}, {
          "command": "editor.action.rename",
          "group": "group_1",
          "when": "config.touchBar.rename"
				}, {
          "command": "workbench.action.closeOtherEditors",
          "group": "group_2",
          "when": "config.touchBar.closeOtherEditors"
				}, {
          "command": "workbench.action.toggleZenMode",
          "group": "group_2",
          "when": "config.touchBar.toggleZenMode"
				}
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Touch Bar Commands",
      "properties": {
        "touchBar.goToReferences": {
          "type": "boolean",
          "default": true,
          "description": "Go to reference [option + click]"
        },
        "touchBar.terminal": {
          "type": "boolean",
          "default": true,
          "description": "Toggle the terminal [on/off]"
        },
        "touchBar.rename": {
          "type": "boolean",
          "default": true,
          "description": "Rename element [F2]"
        },
        "touchBar.closeOtherEditors": {
          "type": "boolean",
          "default": true,
          "description": "Close other editors in group [option + cmd + T]"
        },
        "touchBar.toggleZenMode": {
          "type": "boolean",
          "default": true,
          "description": "Close other editors in group [option + cmd + T]"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
